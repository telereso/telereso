    <declare-styleable name="RemoteEditText">
        <attr name="android:text" />
        <attr name="android:hint" />
    </declare-styleable>

    <declare-styleable name="RemoteButton">
        <attr name="android:text" />
    </declare-styleable>

    <declare-styleable name="RemoteToolbar">
